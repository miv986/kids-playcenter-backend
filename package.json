{
  "name": "kids-playcenter-backend",
  "version": "1.0.0",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/scripts/start-server.js",
    "start:no-migrate": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:watch": "nodemon --watch src --ext ts,js --exec ts-node src/server.ts",
    "postbuild": "npm run generate",
    "generate": "prisma generate",
    "postinstall": "npm run generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:status": "prisma migrate status",
    "migrate:reset": "prisma migrate reset",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:check": "prisma migrate status && prisma validate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "prisma": "^5.22.0",
    "resend": "^6.0.2",
    "typescript": "^5.9.2",
    "@types/bcrypt": "^6.0.0",
    "@types/body-parser": "^1.19.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@types/node-cron": "^3.0.11"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2"
  }
}
